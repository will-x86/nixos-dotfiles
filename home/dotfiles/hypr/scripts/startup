#!/usr/bin/env bash


# Kill already running process
_ps=(waybar mako)
for _prs in "${_ps[@]}"; do
	if [[ `pidof ${_prs}` ]]; then
		killall -9 ${_prs}
	fi
done




~/.config/hypr/scripts/notifications &
swww-daemon & 
swww img ~/.config/hypr/wallpapers/monochrome.jpg & 
wal -i ~/.config/hypr/wallpapers/monochrome.jpg -q -n -e -s -c &
pywalfox update &

~/.config/hypr/scripts/statusbar &

# Start network manager applet
nm-applet --indicator &

# Start mpd
exec mpd &
